{
  "name": "q2-debugger",
  "version": "0.0.1",
  "description": "chrome extension for debugging q2",
  "keywords": [
    "q2-debugger",
    "chrome",
    "dev",
    "tools"
  ],
  "homepage": "https://github.com/codejamninja/q2-debugger",
  "bugs": {
    "url": "https://github.com/codejamninja/q2-debugger/issues",
    "email": "jam@codejam.ninja"
  },
  "license": "MIT",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/codejamninja/q2-debugger"
  },
  "scripts": {
    "build": "mkdir -p dist && cp -r src/public/* dist && webpack",
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/* && jest --clearCache",
    "clean:purge": "jest --clearCache && git clean -fXd",
    "format": "prettier --write ./**/*.{json,md,scss,yaml,yml,js,jsx,ts,tsx} --ignore-path .gitignore",
    "lint": "npm run format && tsc --allowJs --noEmit && eslint --ext .ts,.tsx ./",
    "lint:fix": "npm run format && tsc --noEmit && eslint --ext .ts,.tsx --fix ./",
    "prepublish": "npm run build",
    "start": "parcel src/public/index.html",
    "test": "jest"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "core-js": "^3.6.4",
    "query-string": "^6.11.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-inspector": "^5.0.1",
    "regenerator-runtime": "^0.13.5",
    "table-inspector": "^0.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/chrome": "^0.0.103",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.8",
    "@types/react": "^16.9.31",
    "@types/react-dom": "^16.9.6",
    "@types/react-inspector": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "decode-uri-component": "^0.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-config-jam": "^0.2.20",
    "is-dom": "^1.1.0",
    "is-object": "^1.0.1",
    "is-window": "^1.0.2",
    "jest": "^25.2.4",
    "object-assign": "^4.1.1",
    "parcel": "^1.12.4",
    "prop-types": "^15.7.2",
    "react-is": "^16.13.1",
    "scheduler": "^0.19.1",
    "split-on-first": "^2.0.0",
    "strict-uri-encode": "^2.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "testRegex": "tests(/|/.*/)[^_/]*[jt]sx?$",
    "verbose": true
  }
}
